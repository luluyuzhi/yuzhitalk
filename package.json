{
    "name": "yuzhitalk",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "mocha out/*.test.js",
        "build": "tsc",
        "start": "dapr run --app-id=service-chat --dapr-http-port=3501 --app-port=8080 -- ts-node --project src/tsconfig.json  -r tsconfig-paths/register   src/main.ts",
        "server": "ts-node --project src/tsconfig.json  -r tsconfig-paths/register   src/main.ts",
        "client": "ts-node src/client.ts",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
    },
    "config": {
        "port": 8080
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/luluyuzhi/yuzhitalk"
    },
    "author": "yimin",
    "license": "ISC",
    "dependencies": {
        "@grpc/grpc-js": "^1.3.7",
        "dapr": "0.0.1",
        "dapr-client": "^0.12.0",
        "dotenv": "^10.0.0",
        "log4js": "^6.3.0",
        "mysql2": "^2.3.0",
        "pbjs": "0.0.14",
        "priorityqueue": "^1.0.0",
        "protobufjs": "^6.10.2",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.2.36",
        "typescript": "^4.3.4"
    },
    "devDependencies": {
        "@types/chai": "^4.2.17",
        "@types/mocha": "^8.2.2",
        "@types/node": "^15.0.1",
        "@types/sinon": "^10.0.2",
        "chai": "^4.3.4",
        "jest": "^26.6.3",
        "mocha": "^8.3.2",
        "sinon": "^11.1.1",
        "tsconfig-paths": "^3.9.0"
    }
}
